   "source": [
    "import gdown \n",
    "\n",
    "src_url = input(\"Enter Google Drive share url: \")\n",
    "file_id = src_url.split('=')[1]\n",
    "src_url = \"https://drive.google.com/uc?id={fid}\".format(fid=file_id)\n",
    "url_parse = src_url\n",
    "dst = \"./data.zip\"\n",
    "gdown.download(src_url, dst, quiet=False)"
   ]
  },
